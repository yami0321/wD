import net.md_5.bungee.api.ChatColor;

import org.bukkit.event.Listener;
import org.bukkit.plugin.java.JavaPlugin;

import Command.wD;

public class WD extends JavaPlugin implements Listener {

	public void onEnable(){
		this.getServer().getConsoleSender().sendMessage(ChatColor.GREEN + "wDプラグインを起動しました");
		this.getServer().getPluginManager().registerEvents(this, this);

		getCommand("wD").setExecutor(new wD());
	}

	@Override
	public void onDisable(){
		this.getServer().getConsoleSender().sendMessage(ChatColor.RED + "wDは去っていった");
	}
}
